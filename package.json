{
  "name": "agentflow",
  "description": "AgentFlow - AI Chat Management System",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint:fix": "oxlint-tsgolint --fix",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:setup": "playwright test --project=setup",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:debug": "playwright test tests/e2e/admin/debug-admin-page.spec.ts",
    "test:e2e:reset": "rm -f playwright/.auth/admin.json && npx tsx tests/cleanup-admin-user.ts && playwright test --project=setup",
    "test:e2e:report": "playwright show-report",
    "create-env": "printenv > .env",
    "type-check": "tsc --noEmit",
    "secretlint": "secretlint \"**/*\"",
    "secretlint:fix": "secretlint \"**/*\" --fix",
    "prepare": "husky",
    "db:generate": "echo '⚠️  Use Supabase migrations only: supabase migration new <name>' && exit 1",
    "db:migrate": "echo '⚠️  Use Supabase migrations: supabase db reset (local) or supabase db push (remote)' && exit 1",
    "db:pull": "supabase db pull",
    "db:push": "supabase db push",
    "db:types": "supabase gen types typescript --local > src/lib/supabase/types-generated.ts",
    "db:studio": "supabase studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed:conversations": "tsx tools/scripts/seed-conversations.ts",
    "db:reset": "supabase db reset",
    "get-user-id": "tsx src/scripts/get-user-id.ts",
    "tunnel": "lt --port 3000 --subdomain agentflow-webhook",
    "dev:webhook": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "clean:cache": "node tools/scripts/clean-cache.js",
    "dev:clean": "npm run clean:cache && npm run dev"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.0",
    "@ai-sdk/react": "^3.0.3",
    "@headlessui/react": "^2.2.0",
    "@next/bundle-analyzer": "^15.1.4",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^10.29.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.84.0",
    "@svgr/webpack": "8.1.0",
    "@tabler/icons-react": "^3.19.0",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-table": "^8.21.3",
    "@types/dompurify": "^3.0.5",
    "@types/js-yaml": "^4.0.9",
    "@types/uuid": "^10.0.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "ai": "^6.0.3",
    "ajv": "^8.17.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dompurify": "^3.2.6",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.11.9",
    "google-auth-library": "^9.14.2",
    "isomorphic-dompurify": "^2.26.0",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.536.0",
    "next": "^14.2.33",
    "qss": "^3.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.1",
    "react-window": "^1.8.11",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.0",
    "stripe": "^17.5.0",
    "svix": "^1.73.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@secretlint/secretlint-rule-preset-recommend": "^11.3.0",
    "@snaplet/copycat": "^6.0.0",
    "@snaplet/seed": "^0.98.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-window": "^1.8.8",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.33",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^16.2.7",
    "oxlint-tsgolint": "^0.10.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "secretlint": "^11.3.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.4",
    "typescript": "5.7.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "bash -c 'tsc --project tsconfig.build.json --noEmit'"
    ],
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ]
  }
}
